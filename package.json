{
  "name": "claudetoall_sync",
  "version": "1.0.0",
  "description": "Sync Claude's config to all detected AI agents",
  "keywords": [
    "ai",
    "cli",
    "claude",
    "cline",
    "configuration",
    "copilot",
    "cursor",
    "development",
    "rules"
  ],
  "homepage": "https://github.com/Git-Fg/claudetoall_sync#readme",
  "bugs": {
    "url": "https://github.com/Git-Fg/claudetoall_sync/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Git-Fg/claudetoall_sync.git"
  },
  "license": "MIT",
  "author": "Git-Fg",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "claudetoall_sync": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/cli.ts src/index.ts --format cjs,esm --dts --clean",
    "check": "pnpm run fmt:check && pnpm run oxlint && pnpm run eslint && pnpm run typecheck",
    "cicheck": "pnpm run cicheck:code && pnpm run cicheck:content",
    "cicheck:code": "pnpm run check && pnpm run test",
    "cicheck:content": "pnpm run cspell && pnpm run secretlint",
    "cspell": "cspell --no-progress --gitignore .",
    "dev": "tsx src/cli.ts",
    "eslint": "eslint . --max-warnings 0 --cache",
    "eslint:fix": "eslint . --fix --max-warnings 0 --cache",
    "fix": "pnpm run fmt && pnpm run oxlint:fix && pnpm run eslint:fix",
    "fmt": "oxfmt .",
    "fmt:check": "oxfmt --check .",
    "knip": "knip",
    "oxlint": "oxlint . --max-warnings 0",
    "oxlint:fix": "oxlint . --fix --max-warnings 0",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "secretlint": "secretlint --secretlintignore .gitignore \"**/*\"",
    "sort": "sort-package-json",
    "test": "vitest run --silent=true",
    "test:coverage": "vitest run --coverage --silent=true",
    "test:e2e": "vitest run --config vitest.e2e.config.ts --silent=false",
    "test:watch": "vitest --silent=true",
    "typecheck": "tsgo --noEmit"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "dependencies": {
    "commander": "14.0.3"
  },
  "devDependencies": {
    "@secretlint/secretlint-rule-preset-recommend": "11.3.1",
    "@tsconfig/node24": "24.0.4",
    "@types/node": "25.3.0",
    "@typescript/native-preview": "7.0.0-dev.20260218.1",
    "@vitest/coverage-v8": "4.0.18",
    "cspell": "9.6.4",
    "eslint": "9.39.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-no-type-assertion": "1.3.0",
    "eslint-plugin-oxlint": "1.48.0",
    "eslint-plugin-strict-dependencies": "1.3.31",
    "eslint-plugin-zod-import": "0.3.0",
    "knip": "5.84.1",
    "lint-staged": "16.2.7",
    "oxfmt": "0.33.0",
    "oxlint": "1.48.0",
    "secretlint": "11.3.1",
    "simple-git-hooks": "2.13.1",
    "sort-package-json": "3.6.1",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0",
    "vitest": "4.0.18"
  },
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
